<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/red"/>
        </shape>
    </item>
    <item android:bottom="20dp" android:top="20dp" android:left="20dp" android:right="20dp">
        <shape android:shape="oval">
            <solid android:color="@color/yellow"/>
        </shape>
    </item>
    <item android:bottom="40dp" android:top="40dp" android:left="40dp" android:right="40dp">
        <shape android:shape="ring" android:useLevel="false">
            <solid android:color="@color/blue"/>
        </shape>
    </item><item android:bottom="100dp" android:top="100dp" android:left="100dp" android:right="100dp">
        <shape android:shape="line">
            <stroke android:width="5dp" android:color="@color/green"/>
        </shape>
    </item>
</layer-list>